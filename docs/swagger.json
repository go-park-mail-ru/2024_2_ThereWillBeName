                },
                "description": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "image": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "numberOfReviews": {
                    "type": "integer"
                },
                "phoneNumber": {
                    "type": "string"
                },
                "rating": {
                    "type": "integer"
                }
